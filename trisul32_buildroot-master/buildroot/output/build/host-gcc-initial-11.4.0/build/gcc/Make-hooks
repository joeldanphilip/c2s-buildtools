SERIAL_LIST =
else
SERIAL_LIST = $(wordlist $(DO_LINK_SERIALIZATION),2, lto1 c)
endif
SERIAL_COUNT = 3
INDEX.c = 0
lto1.prev = $(if $(word 2,$(SERIAL_LIST)),$($(word 2,$(SERIAL_LIST)).serial))
INDEX.lto1 = 1
lto2.prev = $(if $(word 1,$(SERIAL_LIST)),$($(word 1,$(SERIAL_LIST)).serial))
INDEX.lto2 = 2
