ifeq ($(DO_LINK_SERIALIZATION),)
SERIAL_LIST =
else
SERIAL_LIST = $(wordlist $(DO_LINK_SERIALIZATION),4, lto1 fortran c++ c)
endif
SERIAL_COUNT = 5
INDEX.c = 0
c++.prev = $(if $(word 4,$(SERIAL_LIST)),$($(word 4,$(SERIAL_LIST)).serial))
INDEX.c++ = 1
fortran.prev = $(if $(word 3,$(SERIAL_LIST)),$($(word 3,$(SERIAL_LIST)).serial))
INDEX.fortran = 2
lto1.prev = $(if $(word 2,$(SERIAL_LIST)),$($(word 2,$(SERIAL_LIST)).serial))
INDEX.lto1 = 3
lto2.prev = $(if $(word 1,$(SERIAL_LIST)),$($(word 1,$(SERIAL_LIST)).serial))
INDEX.lto2 = 4
